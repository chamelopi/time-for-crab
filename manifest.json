{
    "manifest_version": 2,
    "name": "time-for-crab",
    "version": "1.0",
    "description": "Tries to replicate Tumblr's 2022 April Fools crab summoning feature",
    "icons": {
        "48": "icon.png"
    },
    "content_scripts": [
        {
            "matches": ["*://*.tumblr.com/*"],
            "js": ["time-for-crab.js"],
            "css": ["style.css"]
        }
    ]
}